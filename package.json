{
  "name": "youtube-music-electron",
  "description": "Desktop app for Google Music on top of Electron",
  "version": "2.18.0",
  "homepage": "https://github.com/twolfson/youtube-music-electron",
  "author": {
    "name": "Simon Hillbom",
    "email": "simon@internetet.se"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/twolfson/youtube-music-electron.git"
  },
  "license": "Unlicense",
  "main": "lib/youtube-music-electron.js",
  "engines": {
    "node": ">= 6.0.0"
  },
  "bin": {
    "youtube-music-electron": "bin/youtube-music-electron.js"
  },
  "scripts": {
    "start": "node bin/youtube-music-electron.js",
    "build-deps": "node-gyp rebuild --target=v8.11.3",
    "postinstall": "install-app-deps",
    "build-mac": "build -m --x64",
    "build-win": "build -w --x64"
  },
  "build": {
    "productName": "Youtube Music Player",
    "icon": "./icon/icon",
    "appId": "se.internetet.youtubemusicplayer",
    "files": [
      "build/"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "icon": "./icon/icon.icns",
      "target": [
        "zip"
      ],
      "bundleVersion": "1000"
    }
  },
  "dependencies": {
    "bindings": "^1.3.0",
    "commander": "~2.15.1",
    "configstore": "~3.1.2",
    "monogamous": "~1.0.3",
    "nan": "^2.10.0",
    "replify": "~1.2.0",
    "semver": "^5.5.0",
    "underscore": "~1.9.1",
    "winston": "~3.0.0"
  },
  "devDependencies": {
    "electron": "~2.0.2",
    "electron-builder": "^20.15.1",
    "foundry": "~4.4.2",
    "foundry-release-git": "~2.0.2",
    "foundry-release-npm": "~2.0.2",
    "jscs": "~3.0.7",
    "jshint": "~2.9.5"
  },
  "keywords": [
    "youtube",
    "music",
    "electron",
    "atom-shell"
  ]
}
